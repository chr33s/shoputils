		"@types/react": "19.1.2",
		"react": "19.1.0",
		"tsdown": "0.10.2",
		"typescript": "5.8.3",
		"vite": "6.3.1",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.1.1"
	},
	"engines": {
		"node": "^22.14.0",
		"npm": ">=9.6.4"
